@model CursovaMVC.ViewModels.StorageViewModel
@{
    ViewData["Title"] = "StorageView";
}
<environment include="Development">
    <link href="~/css/site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/Storage.css" />
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
</environment>

<div class="row mt-5 mb-2 row d-flex justify-content-center contm">
    @{
        foreach (Storage item in Model.GetStorages)
        {
            @await Html.PartialAsync("_InfoCard", item)
        }

    }
</div>

<h2>StorageView</h2>

