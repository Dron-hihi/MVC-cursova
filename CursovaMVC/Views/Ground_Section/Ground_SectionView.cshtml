@model CursovaMVC.ViewModels.Ground_SectionViewModel
@{
    ViewData["Title"] = "Ground_SectionView";
}
<environment include="Development">
    <link href="~/css/site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/Ground_Section.css" />
    <link href="~/css/StyleSheet.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
</environment>

<div class="row mt-5 mb-2 d-flex justify-content-center contm">
    @{
        foreach (Ground_Section item in Model.GetGround_Sections)
        {
            @await Html.PartialAsync("_InfoCard", item)
        }

    }
</div>

<h2>Ground_SectionView</h2>

